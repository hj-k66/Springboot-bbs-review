{{>layouts/header}}
<div>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Content</th>
        </tr>
        </thead>
        <tbody class="table-group-divider">
        {{#articles}}
            <tr>
                <th>{{id}}</th>
                <td><a href="/articles/{{id}}">{{title}}</a></td>
                <td>{{content}}</td>
            </tr>
        {{/articles}}
        </tbody>
    </table>
</div>
<a href="/articles/new" class="btn btn-primary">새글작성</a>
{{>layouts/footer}}